/* pages/home/home.wxss */
.container {
  display: flex;
}

.title {
  font-size: 40rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.table {
  border: 1px solid #ccc;
  border-collapse: collapse;
}

.header {
  font-weight: bold;
}

.row {
  display: flex;
  border-bottom: 1px solid #ccc;
}

.cell {
  border: 1px solid #ccc;
  flex: 1;
  padding: 20rpx;
  text-align: center;
}
